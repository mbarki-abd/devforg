# DevForge Agent API Configuration

# Server
PORT=8080
HOST=0.0.0.0
NODE_ENV=production
LOG_LEVEL=info

# Authentication
AGENT_API_KEY=your-secure-api-key-here

# Redis
REDIS_URL=redis://localhost:6379

# CORS
CORS_ORIGIN=https://devforge.your-domain.com

# Default Anthropic API Key (optional, can be passed per-request)
ANTHROPIC_API_KEY=

# Default Azure credentials (optional, can be passed per-request)
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_TENANT_ID=

# Default Google Cloud credentials (optional, can be passed per-request)
GOOGLE_APPLICATION_CREDENTIALS=
